<?xml version="1.0"?>
<package>
  <metadata>
    <id>ReSharper.GoToWord</id>
    <version>1.0</version>
    <title>Go to Word</title>
    <authors>Alexander Shvedov</authors>
    <owners>controlflow</owners>
    <projectUrl>https://github.com/controlflow/resharper-gotoword</projectUrl>
    <iconUrl>https://raw.github.com/controlflow/resharper-gotoword/master/Content/icon.png</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>
      Triple [Ctrl]+[T] to search whole solution for any text in any types of project files. Really fast case-insensitive search, powered by ReSharper caches. Standard "find results" dialog with grouping and previews. Can search for text, selected in current file.
    </description>
    <copyright>Copyright © Alexander Shvedov, 2013</copyright>
    <dependencies>
      <dependency id="ReSharper" version="8.0" />
    </dependencies>
    <releaseNotes>
      Integration with "Go to Everything" - triple [Ctrl]+[T] now opens "Go to Word" search. ReSharper 8.2 EAP support. Fix action bug for R# 8.0.
    </releaseNotes>
    <tags>navigation search word words text index cache</tags>
  </metadata>
  <files>
    <file src="GoToWord\bin.R80\$Configuration$\" target="ReSharper\v8.0\plugins\" />
    <file src="GoToWord\bin.R81\$Configuration$\" target="ReSharper\v8.1\plugins\" />
    <file src="GoToWord\bin.R82\$Configuration$\" target="ReSharper\v8.2\plugins\" />
    <file src="GoToWord\bin.R90\$Configuration$\" target="ReSharper\v9.0\plugins\" />
  </files>
</package>